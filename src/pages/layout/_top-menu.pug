.top-panel
  .container
    a(href="#")
      img.logo(src=require('../../img/logo.png'), alt="Betmafia")
    nav.nav
      input.menu-checkbox(type="checkbox" id="menu-checkbox")
      label.menu-button(for="menu-checkbox")
        i.menu-button__icon
      ul.menu
        each val in ['Прогнозы', 'Отзывы', 'Гарантии', 'Франшиза', 'О нас']
          li.menu__item
            label.menu__section(for="menu-checkbox")= val
        li.social-links
          each val in ['v', 't', 'vk', 'i']
            a.social-links__link(href='#')
              i.social-links__icon.social-links__link_=val