.feedback
  color pcolor
  box-sizing border-box
  background-attachment fixed
  background-size cover
  background-position center center
  background-repeat repeat
  background-image url("../img/feedback_bg.jpg")
  &__blackout
    background-image linear-gradient(rgba(0, 0, 0, 0.8) 0%, rgba(25, 77, 77, 0.2) 100%)
    padding 50px 0
    min-height 948px
    box-sizing border-box
  &__title
    caption()
  &__list
    margin 0 60px 60px 60px
    border 1px solid #fff
.slider
  min-height 650px

.tns-inner
  max-width 100%
.tns-controls,
.tns-nav
  display none


